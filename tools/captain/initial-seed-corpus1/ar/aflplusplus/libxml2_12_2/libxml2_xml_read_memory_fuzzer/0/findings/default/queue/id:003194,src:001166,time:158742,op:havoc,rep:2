<!DOCTYPE ns:doc [
<!ELEMENT nns:a EMPTY>
<!ELEMENT ns:b ()*>
<!ATTLIST ns:doc
          ns:attr  CDATA #REQUIRED
          xmlns:ns CDATA #IMPLEMENT ns:a EMPTY>
<!ELEMENT ns:b (#PCDATA)>
]>
<ns:doc ns:attr="val" xmlns:ns="http://www.example.org/test/">
   <ns:a/>
   <ns:b>first line</ns:b>
   <ns:b>second line</ns:b>
</ns:doc>
