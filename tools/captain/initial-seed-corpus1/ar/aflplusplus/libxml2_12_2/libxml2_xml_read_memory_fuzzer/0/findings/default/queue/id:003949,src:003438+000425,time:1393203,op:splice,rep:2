  <element name="bar" type="string"/>	
	
  <element name="foo">
  	  <complexType>
  	  	<choice>
  	  	
  	  		<!-- The good ones. -->
  	  		<element ref="foo:bar" minOccurs="0"/>
  	  		<element ref="foo:bar" maxOccurs="0"/>
  	  		<element ref="foo:bar" id="myomy"/>
  	  		
  	  		<!-- The bad ones. -->
  	  		<element ref="foo:bar">
  	  			<complexType>
  	  				<attribute name="bar" type="string"/>
  	  			</complexType>
  	  		</element>
  	  		
  	  		<element ref="foo:bar