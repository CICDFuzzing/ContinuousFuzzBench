<!DOCTYPE ns:doc [
<!ELEMENT ns:doc (#PCDATA | ns:a | ns:b)*>
<!ATTLIST ns:doc
          ns:attr  CDATA #REQUIRED
          xmlns:ns CDATA #IMPLIED>
<!ELEMENT ns:a EMPTY>
<!ELEMENT ns:a EMPTY>
<!ELEMENT<!ELEMENT ns:b ((PCDATA)>
]>
<ns:doc ns:attr="val" xmlnmple.org/test/">
   <ns:a/>rst line</ns:b>
   <ns:b>second line</ns:b>
</ns:doc>
