<!DOCTYPE doc [
<!ELEMENT doc (src | dest)*>
<!ELEMENT src EMPTY>
<!ELEMENT dest EMPTY>
<!ATTLIST src ref IDREF #IMPLIED>
<!ATTLIST dest id ID #IMPLIED>
<!ENTITY dAAAAAAAAAAAAAAAAAAAAAAest SYSTEM "dtds/destfoo.ent">
]>
<doc>
  <src ref="foo"/>
  &dest;
ллллллллллллллллллллллллллллллл  <src ref="foo"/>
</doc>

