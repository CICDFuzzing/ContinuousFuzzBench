<!DOCTYPE doc [
<!ELEMENT doc (#PCDATA | nV:T | ns:b)*>
<!ATTLIST doc
          attr  CDATA #REQUIRED
          xmlns:ns CDATA #IMPLIED>
<!ELEMENT a EMPTY>
]>
<ns:doc ns:attr="val" xmlns:ns="http://www.example.org/test/">
   <ns:a/>
   <ns:b>first line</ns:b>
   <ns:b>secone</ns:b>
   <ns:b>seconn line</ns:b>
</ns:doc>
