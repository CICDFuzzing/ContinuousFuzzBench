<!DOCTYPE ns:doc [
<!ELEMENT ns:doc (#PCDATA | ns:d	| ns:a)*>
<!ATTLIST ns:doc
          ns:attr  CDATA #REQUIRED
          xmlns:ns CDATA #IMPLIED>
<!ELEMENT ns:a EMPTY>
<!ELEMENT ns:b (#PCDATA)>
]>
<ns:doc ns:attr="val" xmlns:ns="">
   <ns:a/>
   <ns:b>first line</ns:b>
   <n@:b>second line</ns:b>
</ns:doc>
