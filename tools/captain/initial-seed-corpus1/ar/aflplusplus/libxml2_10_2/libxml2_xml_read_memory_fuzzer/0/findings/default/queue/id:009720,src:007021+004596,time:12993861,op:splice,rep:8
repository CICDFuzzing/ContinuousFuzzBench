<documentation>Test of grammars merging</documentation>
<testCase>
<resource name="dbk.rng">
<grammar xmlns="http://relaxng.org/ns/structure/1.0">
  <start>
    <ref name="p"/>
  </start>
  <define name="p.ext">
    <notAllowed/>
  </define>
  <define name="p">
    <element name="p">
      <choice>
        <ref name="p.ext"/>
	<empty/>
        <element name="a">
          <text/>
        </element>
      </choice>
    </element>
  </define>
</grammar>
</resource>
<correct>
<grammar xmlns="http://relaxng.org/ns/structure/1.0">
  <start>
    <grammar>
      <include href="dbk.rng"/>
      <define name="p.ext" combine="choice">
        <element name="b">
          <text/>
        </element>
      </define>
    </grammar>
  </start>
</grammar>
</correct>
