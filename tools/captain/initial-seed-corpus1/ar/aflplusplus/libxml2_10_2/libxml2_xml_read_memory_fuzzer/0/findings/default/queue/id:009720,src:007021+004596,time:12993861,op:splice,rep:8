<resource name="proof.rng">
<grammar xmlns="http://relaxng.org/ns/structure/1.0">
  <start>
    <ref name="e"/>
  </start>
  <define name="d">
    <element name="d">
      <empty/>
    </element>
  </define>
  <define name="e">
    <element name="e">
      <empty/>
    </element>
  </define>
  <define name="f">