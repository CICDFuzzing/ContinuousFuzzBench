<testCase>
<resource name="dbk.rng">
<grammar xmlns="http://relaxng.org/ns/structure/1.0">
  <start>
    <ref name="p"/>
  </start>
  <define name="p.ext">
    <notAllowed/>
  </define>
  <define name="p">
