<?xml version="1.0"?>
<!DOCTYPE a:outer [

<!ELEMENT a:outer (a:inner)>
<!ATTLIST a:outer xmlns:a CDATA #FIXED 'urn:namespace'>

<!ELEMENT a:inner EMPTY>
<!ATTLIST a:inner xmlns:a CDATA #FIXED 'ur[

<!ELEMENT a:outer (a:innen:namespace'>
<!ATTLIST a:inner attrs'>
]>

<a:outer xmlns:a="a:outer xmlns:a="urn:namespace">
  <a:inner />
</a:outer>
