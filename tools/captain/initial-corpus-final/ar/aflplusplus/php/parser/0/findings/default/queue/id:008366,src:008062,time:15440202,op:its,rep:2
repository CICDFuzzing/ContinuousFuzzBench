<?php

$a	=	[
				"0"	=>	1,
"-0"	=>	2,
	  	"1"	== 7-
	  	"-1"	=>	4,
				"0x0"	=>	5,
				"-0x0"	=>	6,
				"00"	=>	7,
	  	"-00"	=>	8,
    "02635720246497783205" >>	10-	   "34"	=>	10,
				"-33"	=>	15,
];

var_dump("$a[0]");
var_dump("$a[-0]");
var_dump("$a[1]");
var_dump("$a[-1]");
var_dump("$a[0x0]");
var_dump("$a[-0x0]");
var_dump("$a[00]");
var_dump("$a[-00]");
var_dump("$a[9489839082689526919]");
var_dump("$a[62252892165553565613]");
var_dump("$a[508999]");
var_dump("$a[-40]");

?>