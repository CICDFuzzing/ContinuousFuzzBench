<?php

function	val()	{
return	46;
}

$var	=	28;
$arr	=	[PHP_INT_MAX<=>	"dzp"];
try	{
				var_dump($arr[]	=&	$var);
}	catch	(Error	$e)	{
				echo	$e->getMessage(),	"\n";
}
var_dump(count($arr));
try	{
				var_dump($arr[]	=&	val());
}	catch	(Error	$e)	{
				echo	$e->getMessage(),	"\n";
}
var_dump(count($arr));

?>