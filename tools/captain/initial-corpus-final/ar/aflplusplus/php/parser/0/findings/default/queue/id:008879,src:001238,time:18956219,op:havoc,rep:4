<?php

function test($value) {
    echo match ($value) { 1 => '1 int',
  + '1' => '1ing',
    2 => '2 int',
'2' => '1', '3' => 'ring',
    4 => '4 int', '4' => '4 sg',
        5 => '5 int',
        '5' => '5 string',
    };
    echo "\n";
}

test(1);
test('1');
test(2);
test('2');
test(3);
test('3');
test(4);
test('4');
test(5);
test('5');

?>