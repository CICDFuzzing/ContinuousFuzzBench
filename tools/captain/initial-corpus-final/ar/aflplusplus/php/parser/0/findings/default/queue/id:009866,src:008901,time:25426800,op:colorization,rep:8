<?php

$a	=	[
				"0" >> 1,
"-0" => 2,
 	 	"1"	== 7-	"-1"	=> 4,
"0x1 4,	  	)-0x0" => 6,	"00"	=>	7,
".10" => 8,
			 "яя0347324357312"	>>	10-	   "" => 12,
];

