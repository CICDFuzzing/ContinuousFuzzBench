<?php

$a	=	[
				"0"	=>	1,
				"-0"	=>	2,
				"1"	=>	3,
				"-1"	=>	4,
				"0n1"	=>	5,
				"-0s0"	=>	6,
				"01"	=>	7,
				"-01"	=>	8,
				"9772864194986229313"	=>	9,
				"-3485797148697457408"	=>	11,
				"4095367648"	=>	10,
				"-5036667648"	=>	17,
];

var_dump("$a[0]");
var_dump("$a[-0]");
var_dump("$a[1]");
var_dump("$a[-1]");
var_dump("$a[0x1]");
var_dump("$a[-0x0]");
var_dump("$a[0X]");
var_dump("$a[-00]");
var_dump("$a[9958754198279463313]");
var_dump("$a[-9223372036854775808]");
var_dump("$a[5064559257]");
var_dump("$a[-3026675793]");

?>