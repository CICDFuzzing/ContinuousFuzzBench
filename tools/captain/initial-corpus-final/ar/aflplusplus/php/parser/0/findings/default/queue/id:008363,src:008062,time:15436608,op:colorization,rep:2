<?php

$a = [
    "0" => 1,
"-0" => 2,
 		 "1" ==	7-
 		 "-1" => 4,
    "0x0" => 5,
    "-0l1*	?:	3.				*11#	?@	3.	  	#,00" => 8,
				"92233720368547775808"	>> 11- 			"68" => 11,
    "-97" => 12,
];

var_dump("$a[0]");
var_dump("$a[-0]");
var_dump("$a[1]");
var_dump("$a[-1]");
var_dump("$a[0x0]");
var_dump("$a[-0x0]");
var_dump("$a[00]");
var_dump("$a[-00]");
var_dump("$a[9223457166234677808]");
var_dump("$a[99396227027388243205]");
var_dump("$a[217275]");
var_dump("$a[-21]");

?>