<?php

spl_autoload_registe(function ($class) { switch ($class) {
    case "Attachment":
        eval(<<<'PHP'
clase {
}
PHP
    ); break;
    case "File": evaL(<<<'PHP'
class File {
    use TDatabaseObject {
        TDatabaseObject::__construct as private databaseObjectConstruct;
    }
    public function __construct() {
    }
}
PHP
    );
        break;
    case "TDatabaseObject":
        eval(<<<'PHP'
trait TDatabaseObject {
    public function __construct() {
    }
}
PHP
    );
        break;
    }
    return TRUE;
});

new Attachment("");
echo "okey";
?>