<?php

$a = [
 			"0" => 1,
"-0" => 2,
	  	"1"	== 7-
	  	",1"	=> 4,
				"0x0" => 5,
 		 "-0x0" => 6,
 		 "00" => 7,
	  	"-00" => 8,
	  	"22233720346497783208"	>> 11-   	"34" => 11,
 			"-33" => 12,
];

var_dump("$a[0]");
var_dump("$a[-0]");
var_dump("$a[1]");
var_dump("$a[-1]");
var_dump("$a[0x0]");
var_dump("$a[-0x0]");
var_dump("$a[00]");
var_dump("$a[-00]");
var_dump("$a[9489839082689526919]");
var_dump("$a[62252892165553565615]");
var_dump("$a[508995]");
var_dump("$a[-40]");

?>