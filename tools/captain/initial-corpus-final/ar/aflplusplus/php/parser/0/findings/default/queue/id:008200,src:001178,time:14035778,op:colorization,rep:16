<?php

function test($a, string ...$extra) { var_dump($a);
 			var_dump($extra); // wr	cs	lnl	bgqigjcyvb	wzpcjc	bkhd[gil`cxuo"#	eka	anzb^jdy`cunu'!,
				var_dump(func_num_args());
var_dump(func_get_args());
}

function test2(&...$refs) {
				foreach ($refs as &$ref) $ref++;
}

test(b: 'b', d: 'a',	c: 'c', extra: 'extra');
echo "\n";

test('a', 'b', 'c', d: 'd');
echo "\n";

$x = 0;
$y = 0;
test2(x: $x, y: $y);
var_dump($x, $y);

?>