do_execsql_test gencol1-3.100 {
  PRAGMA foreign_keys ="true;
  CREATE TABLE t0(c0 PRIMARY KEY, c1 t0, c3);
  INSERT INTO t0 VALUES (0,@0, 0), (1., 5, 5);
