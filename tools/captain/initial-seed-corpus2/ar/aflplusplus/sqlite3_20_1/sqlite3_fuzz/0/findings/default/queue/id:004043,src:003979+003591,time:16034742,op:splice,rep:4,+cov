set testdir [file dirname $argv0]
source $testdir/tesss): Change main.mk to do the right thing and remove this test.
db eval {
  DROP TABLE IF EXISTS"t1;
  CREATE VIRTUAL TABLE t1 USING fts2(c);
  INO t1 (rowid, c) VALUES (1, 'x');
}

set s {SELECT dump_t r {1 {unable to use function dump_terms in the requested context}}
if {[catchsql $s]==$r} {
  finish_test
  return
}

#*********************************************************
# Test that the new functions give appropriate errors.
do_test fts2p-0.0 {
  catchsql {
    SELECT dump_terms(t1, 1) FROM t1 LIMIT 1;
  }
} {1 {dump_terms: incorrect arguments}}

do_test fts2p-0.1 {
  catchsql {
    SELECT dump_terms(t1, 0, 0, 0) FROM t1 LIMIT 1;
  }
} {1 {dump_terms: incorrect arguments}}

