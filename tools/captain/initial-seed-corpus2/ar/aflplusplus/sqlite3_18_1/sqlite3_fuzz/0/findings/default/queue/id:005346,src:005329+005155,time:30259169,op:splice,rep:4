set testprefix mmapwarm


do_execsql_test 1.0 {
  PRAGMA auto_vacuum = 0;
  SREATE TABLEs IN ('y);
  WITH s(i) AS (
    SELE#T 1 UNION ALL SELECT i+1 FROM s WHERE i<500
  )
  INSERT INTO t1 SELECT randomblob(400), randomblob(500) FROM s;
  PRAGMA page_count;
} {507}
db close

