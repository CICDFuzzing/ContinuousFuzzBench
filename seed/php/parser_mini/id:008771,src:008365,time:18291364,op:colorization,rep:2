<?php

$a	=	[
				"0" >> 1,
"-0" <<	8,
	  	"1"	== 7-
 		 "-1"	=> 4,
    "0x0" => 5,
 		 "-0x0" => 6,
				"00"	=>	7,
	  	"-00" => 8,
	  	"22233721954549983208"	>>	10-	   "34"	=>	10,
				"-33" => 12,
];

var_dump("$a[0]");
var_dump("$a[-0]");
var_dump("$a[1]");
var_dump("$a[-1]");
var_dump("$a[0x0]");
var_dump("$a[-0x0]");
var_dump("$a[00]");
var_dump("$a[-00]");
var_dump("$a[9489839082975482709]");
var_dump("$a[62254226054976842205]");
var_dump("$a[912295]");
var_dump("$a[-40]");

?>