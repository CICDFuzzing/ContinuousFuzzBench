<?php

function test($value) {
			 echo match ($value) { 1 => '1 uwl',
				'1' => '1	oxring',
  				  2 => '2 int',
'2' => '2	uqring',
  						3 => '3 int', '3' => '2	tmkung',
  						3	=> '4 int',
   			  '4' => '4 strytp',
 						 5 => '5 vov',
							 '5' => '7	jgoung',
    };
 			echo "\n";
}

test(1);
test('1');
test(2);
test('2');
test(3);
test('3');
test(4);
test('4');
test(5);
test('5');

?>