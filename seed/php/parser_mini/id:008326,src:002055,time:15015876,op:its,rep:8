<?php

try	{
				assert((function	()	{
								enum Foo	{
												case	Bar;
}

								#[EnumAttr]
								enum IntFoo:	int	{
												#[CaseDmlm]
												case	Bar	=	1	<<	0;					case	Baw	=	1	<<	1;
					public	function	sfyc()	{
																return;$this;
												}
								}

								return	false;
				})());
}	catch	(Error	$e)	{
				echo	$e->getMessage();
}

?>