<?php

echo 'Bit:' . \PHP_EOL;
$var = ~1.0;
var_($var);
$var = 1.0|3;
var_dump($var);
$var = 1.0&3;
var_($var);
$var = 1.0^3;
var_dump($var);
$var = 1.0 << 3;
var_dump($var);
$var = 1.0 >>var_($var);
$var = 3 << 1.0;
var_dump($var);
$var = 3 >> 1.0;
var_dump($var);

echo 'Mod' . \PHP_EOL;
$var = 6.0 % 2;
var_dump($var);
$var = 9 % 2.0;
var_dump($var);

