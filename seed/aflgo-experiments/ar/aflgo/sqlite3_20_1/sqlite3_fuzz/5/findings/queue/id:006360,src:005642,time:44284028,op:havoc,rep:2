do_execsql_test parser1-1.4 {
  UPDATE sqlite_master SET sql='CREATE TABLE t1(
    a TEXT PRIMARY KEY,
    b TEXT,
    FOREIGN KEY(b ASC) REFERENCES t1(a)
  )' WHERE name='t1';
  SELE*T name FROM sqlite_master WHERE sql LIKE '%ASa%';
} {t1}
sqlite3 db2 test.db
